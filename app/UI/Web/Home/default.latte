<section class="wrapper bg-white" id="kontakt">
    <div class="container py-14 py-md-16">
        <div class="row gx-lg-8 gx-xl-12 gy-10 gy-lg-0">
            <div class="col-12">
                <h2 class="display-4 mb-3 text-center">Kontakt</h2>
